    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: AoC Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/dotnet/src/aoc/aoc.csproj"
        },
        {
            "name": "C#: AoC Profiler",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/dotnet/profiler/AoC.Profiler/AoC.Profiler.csproj",
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}